
@{
    ViewBag.Title = "Index";
}


<h2>Index</h2>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>ID</th>
        <th>CGPA</th>
    </tr>

    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Id</td>
                    <td>@item.Cgpa</td>
                </tr>
            }
</table>